<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="241.0" prefWidth="249.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="test.testProject.newController">
   <children>
      <Label fx:id="exampleText" text="This is a Test" textAlignment="CENTER" />
      <VBox alignment="CENTER" prefHeight="50.0" prefWidth="210.0">
         <children>
            <Button fx:id="primaryButton" onAction="#populateFonts" text="Populate Combo Box" />
            <Button fx:id="applyButton" mnemonicParsing="false" onAction="#applyFont" text="Apply Font" />
         </children>
      </VBox>
      <ComboBox fx:id="testComboBox" onMouseEntered="#populateFonts" prefWidth="150.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
